{
  "name": "api.typeorm",
  "version": "1.0.5",
  "description": "TypeORM API boilerplate with Express & Webpack",
  "main": "./dist/server.bundle.js",
  "scripts": {
    "start": "webpack --progress --colors --watch --env development",
    "test": "jest --verbose --setupFiles dotenv/config",
    "build": "webpack --colors --env production",
    "postinstall": "npm run migrations:generate && npm run migrations:run",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli",
    "migrations:create": "npm run typeorm:cli -- migration:create -d ./src/db/migrations -n TypeORMAPI",
    "migrations:generate": "npm run typeorm:cli -- migration:generate -d ./src/db/migrations -n TypeORMAPI",
    "migrations:run": "npm run typeorm:cli -- migration:run"
  },
  "keywords": [
    "express",
    "typeorm",
    "jest",
    "webpack",
    "test",
    "api"
  ],
  "author": "Iestyn Dalli",
  "license": "ISC",
  "dependencies": {
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "deepmerge": "1.3.2",
    "express": "4.17.1",
    "figlet": "1.2.3",
    "helmet": "3.18.0",
    "morgan": "1.9.1",
    "pg": "7.10.0",
    "reflect-metadata": "0.1.10",
    "typeorm": "0.2.0",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@types/body-parser": "1.16.0",
    "@types/bunyan": "0.0.36",
    "@types/chai": "4.1.7",
    "@types/chalk": "2.2.0",
    "@types/cors": "2.8.5",
    "@types/deepmerge": "1.3.0",
    "@types/express": "4.0.35",
    "@types/figlet": "1.2.0",
    "@types/helmet": "0.0.43",
    "@types/http-errors": "1.5.34",
    "@types/jest": "24.0.13",
    "@types/mocha": "2.2.40",
    "@types/morgan": "1.7.35",
    "@types/node": "7.0.8",
    "@types/supertest": "2.0.0",
    "@types/winston": "2.4.4",
    "@types/yargs": "6.6.0",
    "chai": "4.2.0",
    "diff": "3.5.0",
    "dotenv-webpack": "1.7.0",
    "fresh": "0.5.2",
    "growl": "1.10.5",
    "jest": "24.8.0",
    "jest-runner-mocha": "0.6.0",
    "json-loader": "0.5.7",
    "karma": "4.1.0",
    "mocha": "6.1.4",
    "mocha-webpack": "1.1.0",
    "nodemon-webpack-plugin": "4.0.8",
    "supertest": "3.0.0",
    "ts-jest": "24.0.2",
    "ts-loader": "6.0.0",
    "ts-node": "8.1.0",
    "typescript": "3.2.1",
    "webpack": "4.22.0",
    "webpack-bundle-analyzer": "3.3.2",
    "webpack-cli": "3.0.8"
  }
}
